<launch>
  <node name="imu" pkg="witmotion_ros" type="witmotion_ros_node" output="screen">
    <rosparam command="load" file="$(find rc_autonomy)/config/imu.yaml" />
  </node>

  <node pkg="tf" type="static_transform_publisher" name="base_link_imu_static_transform"
      output="screen" args="0 0 0 0 0 0 base_link imu 100" />

  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true" output="screen">
    <rosparam command="load" file="$(find rc_autonomy)/config/robot_localization.yaml" />
  </node>

  <node name="encoder_odom" pkg="rc_localization_odometry" type="encoder_odometry_node" output="screen">
    <rosparam command="load" file="$(find rc_autonomy)/config/encoders.yaml" />
  </node>

  <node pkg="tf" type="static_transform_publisher" name="base_link_encoder_static_transform"
      output="screen" args="0 0 0 0 0 0 base_link encoder 100" />
</launch>